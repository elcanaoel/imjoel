{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.+)\\.(css|js|png|jpg|jpeg|gif|svg|ico|json|txt|woff|woff2|ttf|eot|otf)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}